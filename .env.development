# Automatic browser launch on server start
BROWSER=false

# Server bound hostname
HOST=0.0.0.0

# Server bound port
PORT=3000

# Server HTTPS mode
HTTPS=false

# Feature flags
REACT_APP_SHOW_VERSION_NUMBER=true

# API
REACT_APP_API_URL=http://localhost:4000/graphql
